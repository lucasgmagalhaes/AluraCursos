<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" assembly="LojaWeb" namespace="LojaWeb.Entidades">
  <class Name="Venda">
    <id name="Id">
      <generator class="identity"/>
    </id>
    <many-to-one name="Cliente" column="ClienteId"/>

    <bag name="Produtos" table="Venda_Produto">
      <key column="Venda_Id" />
      <many-to-many column="ProdutoId" class="Produto"/>
    </bag>
  
  </class>
</hibernate-mapping>
